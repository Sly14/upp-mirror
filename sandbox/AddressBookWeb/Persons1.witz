<table border="1" id="persons">
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
</tr>
$for(i in PERSON)
	<tr>
	  <td>$i.FIRSTNAME</td>
	  <td>$i.LASTNAME</td>
	  <td>$i.EMAIL</td>
	  <td>
	  	<a href=$Edit(i.ID)>Edit</a>
	  	<a href=$Delete(i.ID)>Delete</a>
	  </td>
	</tr>	
$/
</table>
