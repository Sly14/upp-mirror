<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="cs" lang="cs">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>#TITLE</title>
	<script type="text/javascript">
		function AjaxGet(request) {
			var xmlhttp;
			if(window.XMLHttpRequest)
				xmlhttp = new XMLHttpRequest();
			else
				xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
			xmlhttp.onreadystatechange = function() {
				if(xmlhttp.readyState == 4 && xmlhttp.status == 200) {
					var ss = xmlhttp.responseText.split('\1');
					for(i = 0; i < ss.length; i++) {
						var pos = ss[i].indexOf(':');
						if(pos >= 0)
							document.getElementById(ss[i].slice(0, pos)).innerHTML = ss[i].slice(pos + 1);
					}
				}
			}
			xmlhttp.open("GET", request, true);
			xmlhttp.send();
		}
	</script>
<html>
<body id="body_id">
	#BODY
</body>
</html>

#define TITLE This is an experimental Web AddressBook application
