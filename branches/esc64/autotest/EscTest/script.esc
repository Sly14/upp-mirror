check(a, b)
{
//	Print("// ========================");
//	Print("// a: " + to_string(a) ", b: " + to_string(b));
	Check("a", a, "");
	Check("b", b, "");
	Check("a + b", a + b, "");
	Check("a - b", a - b, "");
	Check("a * b", a * b, "");
	Check("a / b", a / b, "");
}

main() {
	check(0, 1);
	check(1.1, 1.2);
	check(0xfffffffffffffff0, 1);
}
